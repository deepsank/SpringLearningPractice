<?xml version="1.0" encoding="UTF-8"?>

<web-app >

  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
  </welcome-file-list>
<!--  Servlet Declaration-->
  <servlet>
    <servlet-name>first</servlet-name>
    <servlet-class>com.deepsank.servlets.FirstServlet</servlet-class>
  </servlet>
  
<!--  Mapping-->
  <servlet-mapping>
    <servlet-name>first</servlet-name>
    <url-pattern>/web</url-pattern>
  </servlet-mapping>

  <!--  Servlet Declaration-->
  <servlet>
    <servlet-name>servletUsingGenericServlet</servlet-name>
    <servlet-class>com.deepsank.servlets.UsingGenericServlet</servlet-class>
  </servlet>

  <!--  Mapping-->
  <servlet-mapping>
    <servlet-name>servletUsingGenericServlet</servlet-name>
    <url-pattern>/genericServlet</url-pattern>
  </servlet-mapping>

  <!--  Servlet Declaration-->
  <servlet>
    <servlet-name>servletUsingHttpServlet</servlet-name>
    <servlet-class>com.deepsank.servlets.UsingHTTPServlet</servlet-class>
  </servlet>

  <!--  Mapping-->
  <servlet-mapping>
    <servlet-name>servletUsingHttpServlet</servlet-name>
    <url-pattern>/httpServlet</url-pattern>
  </servlet-mapping>

  <!--  Servlet Declaration-->
  <servlet>
    <servlet-name>registerServlet</servlet-name>
    <servlet-class>com.deepsank.servlets.RegisterServlet</servlet-class>
  </servlet>

  <!--  Mapping-->
  <servlet-mapping>
    <servlet-name>registerServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>

  <!--  Servlet Declaration-->
  <servlet>
    <servlet-name>successServlet</servlet-name>
    <servlet-class>com.deepsank.servlets.SuccessServlet</servlet-class>
  </servlet>

  <!--  Mapping-->
  <servlet-mapping>
    <servlet-name>successServlet</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>

  <display-name>Archetype Created Web Application</display-name>
</web-app>
